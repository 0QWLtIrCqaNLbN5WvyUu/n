<script src="common/js/jquery.magnific-popup.js"></script>
<script src="common/js/modalScript.js"></script>
<script src="common/js/script.js"></script>

<script>
	$(function(){
		var _page = 0;
		load_therapists();

		$('a[href="#more_therapists"]').on('click', function(){
			load_therapists();
            return false;
		});
		function load_therapists(){
			$.ajax({
				type: 'GET',
				url: 'therapist.php?salon_id=0&id=104810&kid=0&more',
				cache: false,
				data: {p: _page},
				dataType: 'json'
			}).done(function(resp){
				for(var i in resp.therapists){
					var th = resp.therapists[i];
					var html = '';
					html += '<li class="matchHeight">';
                    //html += '<div class="favBtn" therapist_id="'+th.id+'"></div>';
					html += '<a href="therapist.php?id='+th.id+'">';
					html += '<div class="photo Hv"><img src="contents/therapist/'+th.image1+'" width="339" height="508" alt="セラピスト写真"/></div>';
					html += '<div class="name">';
					html += '<span class="th-name"></span><br>';
					html += '<span class="fs-90"></span>';
					html += '</div>';
					html += '</a>';
					html += '</li>';
					var $dom = $(html);
					$dom.find('.th-name').text(th.name);
					$dom.find('.fs-90').text(th.style);
					if(th.favorite){
						$dom.find('.favBtn').addClass('on');
					}else{
						$dom.find('.favBtn').addClass('off');
					}
					$('.otherTh').append($dom);
				}
				if (resp.count <= 8 || resp.page > 0) {
					$('.viewMore').hide();
				}
			}).fail(function(){
				$('.viewMore').hide();
			});
			_page++;
			return false;
		}
	});
</script>
<script>
$(function(){
    var $setElm = $('.kuchikomi .txt');
    var cutFigure = '100'; // カットする文字数
    var afterTxt = '<span class="abbr"><a href="#">・・・続きを見る</a></span>'; // 文字カット後に表示するテキスト
 
    $setElm.each(function(){
        var textLength = $(this).text().length;
        var textTrim = $(this).text().substr(0,(cutFigure))
 
        if(cutFigure < textLength) {
            $(this).html(textTrim + afterTxt).css({visibility:'visible'});
        } else if(cutFigure >= textLength) {
            $(this).css({visibility:'visible'});
        }
    });
});
</script>
<script src="common/js/smoothscroll.js"></script>
<script>
$(document).on('click', '.favBtn', function(){
	let $btn = $(this);
	let v = 1;
	if($(this).hasClass('on')){
		v = 0;
	}
	let therapist_id = $(this).attr('therapist_id');
	if(v == 0){
		let data = {};
		data.value = 0;
		data.therapist_id = therapist_id;
		$.ajax({
			type: 'POST',
			url: 'therapist.php?id=104810&favorite',
			data: data,
			cache: false,
			dataType: 'json'
		}).done(function(resp){
			if(resp.btn){
				$btn.removeClass('off').addClass('on');
			}else{
				$btn.removeClass('on').addClass('off');
			}
		});
		return;
	}
	$('#addFavModal').attr('therapist_id', therapist_id);
	$.ajax({
		type: 'GET',
		url: 'therapist.php?id=104810&get_favorite',
		data: {therapist_id: therapist_id},
		cache: false,
		dataType: 'json'
	}).done(function(resp){
		if(!resp.result)return;
		$('#addFavModal [therapist_name]').text(resp.therapist_name);
		$('#addFavModal [name="exp"]').prop('checked', false);
		$('#addFavModal [name="exp"][value="'+resp.exp+'"]').prop('checked', true);
		$('#addFavModal [name="memo"]').val(resp.memo);
		$('[name="notification"]').prop('checked', resp.notification!=0);
		$('[href="#addFavModal"]').trigger('click');
	}).fail(function(){
	});
});
$('[href="#favorite_submit"]').on('click', function(e){
	e.preventDefault();
	let therapist_id = $('#addFavModal').attr('therapist_id');
	let $btn = $('[therapist_id="'+therapist_id+'"].favBtn');

	let $parent = $('#addFavModal');

	let data = {};
	data.value = 1;
	data.therapist_id = therapist_id;
	data.exp = $parent.find('input[name="exp"]:checked').val();
	data.memo = $parent.find('textarea[name="memo"]').val();
	data.status = $parent.find('a[href="#status"]').attr('status');
	data.notification = $parent.find('input[name="notification"]').prop('checked') ? 1 : 0;
	$.ajax({
		type: 'POST',
		url: 'therapist.php?id=104810&favorite',
		data: data,
		cache: false,
		dataType: 'json'
	}).done(function(resp){
		if(resp.btn){
			$btn.removeClass('off').addClass('on');
		}else{
			$btn.removeClass('on').addClass('off');
		}
		$.magnificPopup.close();
	});
});
</script>
<script>
$(function(){
	load_comments();
});
function load_comments(){
	let ids = [];
	$('section.Report').each(function(){
		let id = $(this).attr('kuchikomi_id') || 0;
		if(id){
			ids.push(id);
		}
	});
	$.ajax({
		type: 'GET',
		url: 'ajax_comment.php?load',
		data: {ids: ids},
		cache: false,
		dataType: 'json'
	}).done(function(resp){
		let html = '';
		html += '<li>';
		html += '	<h3>';
		html += '	<span class="comment_no"></span>';
		html += '	<span class="badge thank" style="display:none">';
		html += '		<a href="#thankBadgeModal" class="popup-modal">';
		html += '			<figure><img src="common/img/bg-badge-thank.svg" alt=""/></figure>	';
		html += '			<span class="num top-1"></span>';
		html += '		</a>';
		html += '	</span>';
		html += '	<span class="unei" style="display:none">運営</span><a class="name"></a><span class="paid" style="display:none">有料会員</span>のコメント';
		html += '	<span class="gbValue"><img style="display:none;" class="icon-goodbad"/></span>';
		html += '	</h3>';
		html += '	<div class="tx"></div>';
		html += '	<div class="goodBad">';
		html += '		<ul>';
		html += '			<li class="good">';
					html += '				<div class="btn"><a href="#btn_good_comment" k_id=""><img src="common/img/btn-good.png" width="100" height="41" alt="GOOD"/></a></div>';
				html += '				<div class="right">';
		html += '					<span class="count">0</span>';
		html += '					<span class="unit">点</span>';
		html += '				</div>';
		html += '			</li>';
		html += '			<li class="bad">';
					html += '				<div class="btn"><a href="#btn_bad_comment" k_id=""><img src="common/img/btn-bad.png" width="100" height="41" alt="BAD"/></a></div>';
				html += '				<div class="right">';
		html += '					<span class="count">0</span>';
		html += '					<span class="unit">点</span>';
		html += '				</div>';
		html += '			</li>';
		html += '			<li class="users" style="display:none">';
		html += '				<a href="#open_usersModal" k_id=""><img src="common/img/icon-users.svg" alt=""/></a>';
		html += '			</li>';
		html += '			<li class="reply">';
					html += '				<a href="#btn_reply_comment" k_id=""><div class="repBtn">返信</div></a>';
				html += '			</li>';
		html += '		</ul>';
		html += '		<a href="#doInfoModal2"><div class="outBtn">違反報告</div></a>';
		html += '	</div>';
		html += '</li>';
		for(let i in resp){
			let kuchikomi_comment = resp[i];
			let $parent = $('section[kuchikomi_id="'+kuchikomi_comment.kuchikomi_id+'"].Report');
			let $commentBlock = $parent.find('.userComBlock');
			if($commentBlock.length == 0)continue;
			let $ul = $commentBlock.find('ul').empty();
			if(kuchikomi_comment.comments.length <= 2)
			{
				if($parent.find('.grad-trigger').length > 0){
					$parent.find('.grad-trigger').remove();
					$parent.find('.grad-btn').remove();
					$commentBlock.unwrap().unwrap();
				}
			}
			for(let j in kuchikomi_comment.comments){
				let comment = kuchikomi_comment.comments[j];
				let $dom = $(html);
				if(comment.action == '1'){
					$dom.find('.icon-goodbad').attr('src', 'common/img/icon-good.svg').show();
				}else if(comment.action == '2'){
					$dom.find('.icon-goodbad').attr('src', 'common/img/icon-bad.svg').show();
				}
				$dom.find('.good .count').text(comment.good);
				$dom.find('.bad .count').text(comment.bad);
				$dom.find('[href="#open_usersModal"]').attr({k_id: comment.kuchikomi_id, comment_id:comment.comment_id});
				$dom.attr({comment_id:comment.comment_id});
				if(comment.member_id){
					$dom.find('a.name').attr('href', './userpage.php?user_id='+comment.member_id);
				}
				if(comment.comment_no){
					$dom.find('span.comment_no').text(comment.comment_no+'. ');
				}
				if(comment.good || comment.bad){
					$dom.find('.users').show();
				}
				$dom.find('a.name').text(comment.member_name+'さん');
				$dom.find('div.tx').html(comment.comment);
				if(comment.comment == '' && comment.mosaic){
					$dom.find('div.tx').html('<img width="770" height="166" alt="モザイク" src="'+comment.mosaic+'">');
					$dom.find('li.good, li.bad, li.users, [href="#doInfoModal2"]').hide();
					$dom.find('[href="#btn_reply_comment"]').hide();
					$dom.find('[href="#commentBanModal"]').hide();
				}
				$dom.find('a[href="#doInfoModal2"]').on('click', on_info_modal);
				$dom.find('[k_id]').attr('k_id', comment.kuchikomi_id);
				if(comment.member_recv_thank){
					$dom.find('.badge').css('display', 'inline-block').find('.num').text(comment.member_recv_thank);
				}
				if(comment.member_paid){
					$dom.find('.paid').show();
				}
				if(comment.member_type == 2){
					$dom.find('.unei').show();
				}
				$ul.append($dom);
			}

			if(kuchikomi_comment.comments.length == 0){
				$commentBlock.hide();
			}else{
				$commentBlock.show();
			}
		}
		initMagnificPopup2();
	});
}
function initMagnificPopup2(){
	$('.popup-modal').magnificPopup({
		type: 'inline',
		preloader: false,
		modal: true,
		removalDelay: 300,
		mainClass: 'mfp-fade'
	});
}
$('button[name="submit_btn"]').on('click', function(){
	let $parent = $(this).closest('section.Report');
	let data = {};
	data.kuchikomi_id = $parent.attr('kuchikomi_id');
	data.comment = $parent.find('textarea[name="comment"]').val() || '';
	data.goodbad = $('#ac-'+data.kuchikomi_id).attr('goodbad');
	data.comment_id = $('#ac-'+data.kuchikomi_id).attr('comment_id');
	data.goodbad_id = $('#ac-'+data.kuchikomi_id).attr('goodbad_id');
	if(data.comment == '')return;

	$parent.find('textarea[name="comment"]').val('');
	$parent.find('.ac-box label').trigger('click');
	$.ajax({
		type: 'POST',
		url: 'ajax_comment.php?submit',
		data: data,
		cache: false,
		dataType: 'json',
	}).done(function(resp){
		load_comments();
	});
});
function on_info_modal(e){
	e.preventDefault();
	let $comment = $(this).closest('li');
	$('.fail').text( $comment.find('div.tx').text() );
	$('textarea[name="detail"]').val('');
	$('#infoModal').attr('petit', 0);
	$('a[href="#infoModal"]').attr({'comment_id':$comment.attr('comment_id')}).trigger('click');
}
</script>
<script src="common/js/goodbad.js"></script>
<script>
	$(function(){
		new GoodBad();
		new GoodBad({kind: 'petit'});
	});
</script>
<script>
	$('.grad-trigger').on('change', function(){
		$('.ac-box2').show();
	});

	
	$('button[name="petit_submit_btn"]').on('click', function(e){
		e.preventDefault();

		
		let $parent = $(this).closest('.ac-box');
		let data = {};
		data.all = true;
		data.therapist_id = 104810;
		data.comment = $parent.find('textarea[name="petit_comment"]').val()||'';
		data.mask = $parent.find('input[name="mosaic"]').prop('checked') ? 1 : 0;
		if(data.comment == ''){
			return;
		}
		$parent.find('textarea[name="petit_comment"]').val('');
		$parent.find('input[type="checkbox"]').prop('checked', false);

		$.ajax({
			type: 'POST',
			url: 'api/petit-kuchikomi.php?kuchikomi',
			data: data,
			cache: false,
			dataType: 'json'
		}).done(function(resp){
			$('.commentList').show();
			$('ul.kuchikomiList').closest('.userComBlock').empty().append(resp.html);
		}).fail(function(){
		});
	});
	$(document).on('click', 'a[href="#doInfoModal"]', function(e){
		e.preventDefault();
		let comment_id = $(this).closest('li').attr('comment_id');
		$('textarea[name="detail"]').val('');
		$('#infoModal').attr('petit', 1);
		$('a[href="#infoModal"]').attr('comment_id', comment_id).trigger('click');
	});
	$('a[href="#send_submit"]').on('click', function(e){
		e.preventDefault();
		if($('#infoModal').attr('petit') == '1'){
			let data = {};
			data.comment_id = $('a[href="#infoModal"]').attr('comment_id');
			data.detail = $('textarea[name="detail"]').val()||'';
			if(data.detail == '')return;
			$.ajax({
				type:'POST',
				url: 'api/petit-kuchikomi.php?fail',
				data: data,
				cache: false
			}).always(function(){
				alert("削除依頼を受理しました。\nおおよそ72時間でいただきました削除依頼を精査致しますので今しばらくお待ちください。");
			});
			$.magnificPopup.close();
		}else{
			let data = {};
			data.therapist_id = 104810;
			data.comment_id = $('a[href="#infoModal"]').attr('comment_id');
			data.detail = $('textarea[name="detail"]').val()||'';
			if(data.detail == '')return;
			$.ajax({
				type:'POST',
				url: 'ajax_comment.php?fail',
				data: data,
				cache: false
			}).always(function(){
				alert("削除依頼を受理しました。\nおおよそ72時間でいただきました削除依頼を精査致しますので今しばらくお待ちください。");
			});
			$.magnificPopup.close();
		}
		
	});
</script>
<script>
	$(document).on('click', '[href="#doBanModal"]', function(e){
		e.preventDefault();
		let kuchikomi_id = $(this).closest('[kuchikomi_id]').attr('kuchikomi_id');
		$.getJSON('api/kuchikomi.php?get', {kuchikomi_id: kuchikomi_id}, function(resp){
			$('#banModal').attr({kuchikomi_id: resp.kuchikomi_id, token: resp.token});
			$('#banModal .kuchikomi_therapist_name').text(resp.kuchikomi_therapist_name);
			$('#banModal .kuchikomi_member_name').text(resp.kuchikomi_member_name);
			$('#banModal .member_name').text(resp.member_name);
			$('#banModal textarea[name="detail"]').val('');
			$('a[href="#banModal"]').trigger('click');
		});
	});
	$('a[href="#ban_submit"]').on('click', function(e){
		e.preventDefault();
		let data = {};
		data.kuchikomi_id = $('#banModal').attr('kuchikomi_id')||'';
		data.token = $('#banModal').attr('token')||'';
		data.detail = $('#banModal textarea[name="detail"]').val()||'';
		if(data.detail == ''){
			return;
		}

		$.ajax({
			type: 'POST',
			url: 'api/kuchikomi.php?ban',
			data: data,
			cache: false,
			dataType: 'json'
		}).done(function(resp){
			alert("通報を受理しました。");
			$.magnificPopup.close();
		});
	});
</script>
